{
  "name": "package-auto-version",
  "version": "1.2.2",
  "description": "Automatically update version and update changelog",
  "main": "index.js",
  "scripts": {
    "pav": "./bin/pav",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://cinada@bitbucket.org/cinada/package-auto-version.git"
  },
  "keywords": [
    "package",
    "version",
    "changelog",
    "readme",
    "auto"
  ],
  "author": "Clark A Inada",
  "license": "MIT",
  "homepage": "https://bitbucket.org/cinada/package-auto-version#readme",
  "bin": {
    "pav": "./bin/pav"
  },
  "pav": {
    "changelog": "<rootDir>/CHANGELOG.md",
    "changelogTemplate": "<rootDir>/git-documentation",
    "noPrompt": true,
    "versions": "minor,major,patch",
    "match": "<rootDir>/git-documentation-match"
  },
  "dependencies": {
    "craydent": "^0.9.0"
  }
}
